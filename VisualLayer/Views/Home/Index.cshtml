@using Entities
@using Shared
@{
    ViewData["Title"] = "Home Page";
    DataResponse<Funcionario> dataResponse = ViewBag.Funcionarios;
}
<script>
    function Clicar() {
        var valor1 = document.getElementById("valor1");
        if (valor1 == null)
        {
            valor1 = "";
        }
        var elemento1 = document.getElementById("Elemento1");
        var elemento2 = document.getElementById("Elemento2");
        var abacate = @dataResponse.Data.Count;
        elemento1.innerHTML = valor1;
        elemento2.innerHTML = abacate;
    }

</script>


<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
    <input type="text" id="valor1" size="20" /><br>
    <input type="text" id="valor2" size="20" /><br>
    <button1 type="submit" onclick="Clicar()">Clicar</button1>
    <h2 id="Elemento1"></h2>
    <h2 id="Elemento2"></h2>
</div>
